#!/bin/sh
################################################################################
# runit stage 2 script
################################################################################

set -a
PATH=/usr/bin:/usr/sbin:/bin:/sbin:/usr/X11R6/bin
set +a

################################################################################

rm -frv /.runit/runsvdir/current
ln -fsv maint /.runit/runsvdir/current

runsvchdir /.runit/runsvdir/maint
runsvdir /.runit/runsvdir/current ................................................................................................................................

exit 111
################################################################################
# end of file
################################################################################
